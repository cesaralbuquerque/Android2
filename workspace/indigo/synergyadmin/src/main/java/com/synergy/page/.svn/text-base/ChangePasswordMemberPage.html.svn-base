<html xmlns:wicket="http://wicket.apache.org">
    <wicket:extend>
		<p class="hd"><span><img src="images/back.jpg" alt="" class="mid-img" /> &nbsp; <a wicket:id="profilePassLink" href="#">Profile</a></span><img src="images/change-password.jpg" alt="" /></p>
          
          
          <form wicket:id="edtProfForm" class="memberForm">
    			<span wicket:id="profFeedback"></span><br>
          <div class="le-cal-form">          
            <div class="profile-left">            
                        <label>Old Password</label><br />
                        <input name="" wicket:id="oldpassword" size="30" type="password" /><br />
            </div>
			<div class="clearflot"></div>
                    <div class="profile-left">
                        <label>New Password</label><br />
                        <input name="" wicket:id="password" size="30" type="password" /><br />
            </div>
                    <div class="clearflot"></div>                
                    
                    
                    
                    <div class="profile-left">
                        <label>Retype New Password</label><br />
                        <input wicket:id="repassword" size="30" type="password" /><br />
                    </div>
                     <div class="clearflot"></div>
                    <input type="image" src="images/save-changes.jpg" name="saveProfile" style="width:138px; height:30px"/>
                    
          </div>          
          <div class="clearflot"></div>
          </form>                 	
</wicket:extend>
</html>