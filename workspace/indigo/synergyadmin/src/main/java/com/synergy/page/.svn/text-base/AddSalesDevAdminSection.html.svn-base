<html xmlns:wicket="http://wicket.apache.org">
    <wicket:extend>
	<div>
	<span wicket:id="titlemsg" style="font-size:20px;font-weight:bold"></span>
	<form wicket:id="addform">
	<span wicket:id="feedback"></span>
	Name <input wicket:id="name"><br>
	Email <input wicket:id="email"><br>
	Password <input type="password" wicket:id="pass"><br>
	Retype Password <input type="password" wicket:id="repass"><br>
	<input type="submit" value="Save">
	</form>	
	</div>
</wicket:extend>
</html>

