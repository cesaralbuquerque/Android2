<html xmlns:wicket="http://wicket.apache.org">
	<body style="background-image:url(../img/vert-bg2.gif); background-repeat: repeat-x;">
	<div id="logoWrapper">
    	<br>
    	<img src="img/syn-logo.gif" alt="synergy chat"/> Admin Area    
  	</div>
	<hr>
	<form wicket:id="login">
	<span wicket:id="feedback"></span><br>
	Email: <input wicket:id="email"><br>
	Password: <input wicket:id="pass" type="password"><br>
	<input type="submit" value="Login">
	</form>
	
	<span wicket:id="panel"> </span>
	<wicket:fragment wicket:id="fragmain">
	<div style="float:left;border-right:1px solid black">
		
	<span wicket:id="pmenu"></span>
	<wicket:fragment wicket:id="fragsales">
	<span>Salesperson<br></span>
	<ul>
		<li><a href="#" wicket:id="addsales">Add Salesperson</a></li>
		<li><a href="#" wicket:id="viewsales">View Salespersons</a></li>	
	</ul>
	<br>
	</wicket:fragment>
	<span wicket:id="pmenu2"></span>
	<wicket:fragment wicket:id="fragdeveloper">
	<span>Developer</span>
	<ul>
		<li><a href="#" wicket:id="adddeveloper">Add Developers</a></li>
		<li><a href="#" wicket:id="viewdeveloper">View Developers</a></li>	
	</ul>
	<br>
	</wicket:fragment>
	<span wicket:id="pmenu3"></span>
	<wicket:fragment wicket:id="fragcustomer">
	<span>Customer</span>
	<ul>
		<li><a href="#" wicket:id="addcustomer">Add Customer</a></li>
		<li><a href="#" wicket:id="viewcustomer">View Customers</a></li>	
	</ul>
	<br>
	</wicket:fragment>
	<span wicket:id="pmenu4"></span>
	<wicket:fragment wicket:id="fragmonitor">
	<span>Monitor</span>
	<ul>
		<li><a href="#" wicket:id="viewmonitor">View Reports</a></li>	
	</ul>
	<br>
	</wicket:fragment>
	<a wicket:id="logout">Logout</a>
	</div>
	<div style="margin-left: 150px">
	<wicket:child/>
	</div>
	</wicket:fragment>
