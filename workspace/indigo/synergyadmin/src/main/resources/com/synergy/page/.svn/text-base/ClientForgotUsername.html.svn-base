<html xmlns:wicket="http://wicket.apache.org">
    <wicket:extend>
   <div style="text-align:center">
    		<br/><br/>
    		<div style="text-align:left;padding-left:250px">
    		    		
    		<span wicket:id="feedback"></span>
    		<form wicket:id="form" class="memberForm">
    			
				<p>Type your company website in the field below<br>
    			You will get by email your username.</p>
    			
    			<div class="spacer"></div>
    			
    			<div class="description">Company Website</div>
    			<div class="inputArea">
    				<input type="text" wicket:id="website" class="borderGray" size="30" value="http://" name="website"/>
    			</div>
    			<div class="spacer"></div>
    			
    			<div class="description">&nbsp;</div>
    			<div class="inputArea">
    				<input type="submit" name="submit" value="Confirm"/>
    			</div>
    			<div class="spacer"></div>	
    		</form>

    		</div>

    	</div>
    	
</wicket:extend>
</html>
